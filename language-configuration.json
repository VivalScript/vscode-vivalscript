{
    "comments": {
        "lineComment": "#",
        "blockComment": [ "\"\"\"", "\"\"\"" ]
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        {
            "open": "'",
            "close": "'",
            "notIn": ["comment"]
        },
        [ "\"\"\"", "\"\"\"" ]
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ],
    "onEnterRules": [
        {
            "beforeText": "^\\s*(?:[0-9]*)\\s*>[^#]*?:",
            "action": {
                "indent": "indent"
            }
        }
    ]
}